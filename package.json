{
  "name": "tenstorrent-developer-extension",
  "displayName": "Tenstorrent Developer Extension",
  "description": "Setup and development tools for Tenstorrent hardware",
  "version": "0.0.1",
  "publisher": "tenstorrent",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "tenstorrent.openSetup",
        "title": "Open Tenstorrent Setup",
        "category": "Tenstorrent"
      },
      {
        "command": "tenstorrent.runCommand",
        "title": "Run Tenstorrent Command"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tenstorrent-setup",
          "title": "Tenstorrent Setup",
          "icon": "$(circuit-board)"
        }
      ]
    },
    "views": {
      "tenstorrent-setup": [
        {
          "id": "tenstorrent-checklist",
          "name": "Setup Progress",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "typescript": "^5.4.0",
    "vsce": "^2.15.0"
  }
}


